{
  "openapi": "3.0.0-rc2",
  "servers": [
    {
      "url": "http://petstore.swagger.io/v2"
    }
  ],
  "info": {
    "version": "1.0.0",
    "title": "Swagger Petstore"
  },
  "tags": [
    {
      "name": "pet",
      "description": "Everything about your Pets",
      "externalDocs": {
        "description": "Find out more",
        "url": "http://swagger.io"
      }
    },
    {
      "name": "store",
      "description": "Access to Petstore orders"
    },
    {
      "name": "user",
      "description": "Operations about user",
      "externalDocs": {
        "description": "Find out more about our store",
        "url": "http://swagger.io"
      }
    }
  ],
  "paths": {
    "/pet": {
      "post": {
        "tags": ["pet"],
        "summary": "Add a new pet to the store",
        "description": "",
        "operationId": "addPet",
        "parameters": [],
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["name", "photoUrls"],
                "properties": {
                  "category": {
                    "type": "object",
                    "description": "type of pet",
                    "properties": {
                      "id": {
                        "type": "integer",
                        "format": "int64",
                        "description": "number of pet"
                      },
                      "name": {
                        "type": "string",
                        "description": "name of pet"
                      }
                    },
                    "xml": {
                      "name": "Category"
                    }
                  },
                  "status": {
                    "type": "boolean",
                    "description": "pet status in the store"
                  }
                },
                "xml": {
                  "name": "Pet"
                }
              }
            },
            "application/xml": {
              "schema": "~paths~/pet~post~requestBody~content~application/json~schema"
            }
          },
          "description": "Pet object that needs to be added to the store",
          "required": true
        }
      }
    }
  },
  "x-explorer-enabled": true,
  "x-samples-enabled": true,
  "x-samples-languages": ["curl", "node", "ruby", "javascript", "python"],
  "components": {
    "schemas": {
      "Order": "~paths~/store/order~post~responses~200~content~application/xml~schema",
      "Category":
        "~paths~/pet~post~requestBody~content~application/json~schema~properties~category",
      "User": "~paths~/user~post~requestBody~content~application/json~schema",
      "Tag": "~paths~/pet~post~requestBody~content~application/json~schema~properties~tags~items",
      "Pet": "~paths~/pet~post~requestBody~content~application/json~schema",
      "ApiResponse":
        "~paths~/pet/{petId}/uploadImage~post~responses~200~content~application/json~schema"
    },
    "responses": {},
    "parameters": {},
    "examples": {},
    "requestBodies": {
      "Pet": "~paths~/pet~post~requestBody",
      "UserArray": "~paths~/user/createWithArray~post~requestBody"
    },
    "headers": {}
  }
}
